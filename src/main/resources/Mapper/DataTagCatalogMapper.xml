<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.glmht.excelDemo.DataTagCatalogDao">

	<!-- 通过<resultMap>映射实体类属性名和表的字段名对应关系 -->
	<resultMap id="DataTagCatalogMapping" type="DataTagCatalog">
		<!-- id属性来映射主键字段 -->
		<id column="id" property="id" javaType="Long" />
		<!-- result属性映射非主键字段 -->
		<result column="name" property="name" javaType="String" />
		<result column="number" property="number" javaType="String" />
		<result column="number_type" property="numberType" javaType="String" />
		<result column="parent_number" property="parentNumber" javaType="String" />
		<result column="parent_numbers" property="parentNumbers" javaType="String" />
		<result column="sort" property="sort" javaType="Integer" />
		<result column="level" property="level" javaType="Integer" />
		<result column="business_table" property="businessTable" javaType="String" />
		<result column="create_by" property="createBy" javaType="String" />
		<result column="create_date" property="createDate" javaType="Date" />
		<result column="update_by" property="updateBy" javaType="String" />
		<result column="update_date" property="updateDate" javaType="Date" />
		<result column="version" property="version" javaType="String" />
		<result column="del_flag" property="delFlag" javaType="String" />
		
	</resultMap>
	<sql id="Base_Column_List">
		id,
		name,
		number,
		number_type,
		parent_number,
		parent_numbers,
		sort,
		level,
		business_table,
		create_by,
		create_date,
		update_by,
		update_date,
		version,
		del_flag
	</sql>
	<sql id="insert_Column_List">
		name,
		number,
		number_type,
		parent_number,
		parent_numbers,
		sort,
		level,
		business_table,
		create_by,
		create_date,
		update_by,
		update_date,
		version,
		del_flag
	</sql>
	<select id="selectDataTagCatalogAll" resultMap="DataTagCatalogMapping">
		select
		<include refid="Base_Column_List"></include>
		from data_tag_catalog
	</select>
	<select id="selectDataTagCatalogById" parameterType="Long">
		select
		<include refid="Base_Column_List"></include>
		from data_tag_catalog where id = #{id}
	</select>
	<insert id="insertDataTagCatalog" parameterType="DataTagCatalog" useGeneratedKeys="true" keyProperty="id"> 
		insert into data_tag_catalog
		(
		<include refid="insert_Column_List"></include>
		)
		values(
		#{name},
		#{number},
		#{numberType},
		#{parentNumber},
		#{parentNumbers},
		#{sort},
		#{level},
		#{businessTable},
		#{createBy},
		now(),
		#{updateBy},
		now(),
		#{version},
		#{delFlag}
		)
	</insert>
	<delete id="delDataTagCatalog" parameterType="Long">
	    DELETE from data_tag_catalog where id=#{id}
	</delete>

</mapper>